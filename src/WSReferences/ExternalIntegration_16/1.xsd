<xsd:schema xmlns:tns="http://www.dikom.ru/lift/integration/services/"
		xmlns:xsd="http://www.w3.org/2001/XMLSchema"
		attributeFormDefault="unqualified"
		targetNamespace="http://www.dikom.ru/lift/integration/services/">
	<!-- GENERAL -->
	<xsd:simpleType name="ledalphabet">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="([a-zA-ZЁёа-яА-Я 0-9_=!@#№,\.\-\?%\*\(\)\+&amp;&gt;&lt;&quot;'°\\/|])*"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- 
         Unicode specials
         &#8304; &#185; &#178; &#179; &#8308 - &#8313; - superscript numbers (0-9)
         &#188 - &#190; &#8528 - &#8542; - fraction signs (¼-¾; ⅐-⅞)
     -->
	<xsd:simpleType name="alphanumeric50">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="([a-zA-ZЁёа-яА-Я 0-9\-])*"/>
			<xsd:maxLength value="50"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="alphanumeric20">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="([a-zA-ZЁёа-яА-Я0-9\-])*"/>
			<xsd:maxLength value="20"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Simple patterns for basic date/time string format. 
            Do not validate that string actually represents a date. -->
	<xsd:simpleType name="alphanumericspecials50">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="([a-zA-ZЁёа-яА-Я 0-9_=!@#№$®™`’″«»“”√⌀øØ\^≤≥—±÷≠×·‑ΔδöȽɰɛ,;:~\.–\-\?%\*\(\)\[\]\{\}\+&amp;&gt;&lt;&quot;'°˚\\/|⁰¹²³⁴-⁹¼-¾⅐-⅞])*"/>
			<xsd:maxLength value="50"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- User Service -->
	<xsd:simpleType name="alphanumericspecials100">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="([a-zA-ZЁёа-яА-Я 0-9_=!@#№$®™`’″«»“”√⌀øØ\^≤≥—±÷≠×·‑ΔδöȽɰɛ,;:~\.–\-\?%\*\(\)\[\]\{\}\+&amp;&gt;&lt;&quot;'°˚\\/|⁰¹²³⁴-⁹¼-¾⅐-⅞])*"/>
			<xsd:maxLength value="100"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="alphanumericspecials150">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="([a-zA-ZЁёа-яА-Я 0-9_=!@#№$®™`’″«»“”√⌀øØ\^≤≥—±÷≠×·‑ΔδöȽɰɛ,;:~\.–\-\?%\*\(\)\[\]\{\}\+&amp;&gt;&lt;&quot;'°˚\\/|⁰¹²³⁴-⁹¼-¾⅐-⅞])*"/>
			<xsd:maxLength value="150"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="latinalphanumericspecials50">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="([a-zA-Z 0-9_=!@#№$®™`’″«»“”√⌀øØ\^≤≥—±÷≠×·‑ΔδöȽɰɛ,;:~\.–\-\?%\*\(\)\[\]\{\}\+&amp;&gt;&lt;&quot;'°˚\\/|⁰¹²³⁴-⁹¼-¾⅐-⅞])*"/>
			<xsd:maxLength value="50"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="login">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="([a-zA-ZЁёа-яА-Я0-9\-\._])*"/>
			<xsd:maxLength value="50"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="date">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[\d]{2}.[\d]{2}.[\d]{4}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!--Storage Service-->
	<xsd:simpleType name="time">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[\d]{2}:[\d]{2}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="roleType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="ADMIN"/>
			<xsd:enumeration value="OPERATOR"/>
			<xsd:enumeration value="ENGINEER"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="rfid">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="255"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:element name="UploadUserList">
		<xsd:complexType>
			<xsd:sequence maxOccurs="unbounded"
					minOccurs="0">
				<xsd:element name="user"
						type="tns:UploadUser"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- Item service -->
	<xsd:complexType name="UploadUser">
		<xsd:sequence>
			<xsd:element name="userId"
					type="tns:alphanumeric50"/>
			<xsd:element name="firstName"
					type="tns:alphanumeric50"/>
			<xsd:element name="lastName"
					type="tns:alphanumeric50"/>
			<xsd:element minOccurs="0"
					name="middleName"
					nillable="true"
					type="tns:alphanumeric50"/>
			<xsd:element minOccurs="0"
					name="position"
					nillable="true"
					type="tns:alphanumericspecials50"/>
			<xsd:element name="login"
					type="tns:login"/>
			<xsd:element minOccurs="0"
					name="password"
					nillable="true"
					type="xsd:string"/>
			<xsd:element minOccurs="0"
					name="role"
					nillable="true"
					type="tns:roleType"/>
			<xsd:element minOccurs="0"
					name="rfid"
					nillable="true"
					type="tns:rfid"/>
			<xsd:element name="active"
					type="xsd:boolean"/>
			<xsd:element minOccurs="0"
					name="remoteLogin"
					nillable="true"
					type="tns:login"/>
			<xsd:element minOccurs="0"
					name="remotePassword"
					nillable="true"
					type="xsd:string"/>
			<xsd:element minOccurs="0"
					name="remoteAddress"
					nillable="true"
					type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element name="GetUserList">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:choice>
					<xsd:sequence>
						<xsd:element maxOccurs="1"
								minOccurs="0"
								name="userId"
								nillable="true"
								type="tns:alphanumeric50"/>
					</xsd:sequence>
					<xsd:sequence>
						<xsd:element maxOccurs="1"
								minOccurs="0"
								name="login"
								nillable="true"
								type="tns:login"/>
					</xsd:sequence>
				</xsd:choice>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="GetUserLogList">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:sequence>
					<xsd:element maxOccurs="1"
							minOccurs="0"
							name="userId"
							nillable="true"
							type="tns:alphanumeric50"/>
				</xsd:sequence>
				<xsd:sequence>
					<xsd:element maxOccurs="1"
							minOccurs="0"
							name="login"
							nillable="true"
							type="tns:login"/>
				</xsd:sequence>
				<xsd:sequence>
					<xsd:element maxOccurs="1"
							minOccurs="0"
							name="startDate"
							nillable="true"
							type="xsd:date"/>
				</xsd:sequence>
				<xsd:sequence>
					<xsd:element maxOccurs="1"
							minOccurs="0"
							name="endDate"
							nillable="true"
							type="xsd:date"/>
				</xsd:sequence>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="GetUserListResult">
		<xsd:complexType>
			<xsd:sequence maxOccurs="unbounded"
					minOccurs="0">
				<xsd:element name="user"
						type="tns:GetUserResult"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="GetUserLogListResult">
		<xsd:complexType>
			<xsd:sequence maxOccurs="unbounded"
					minOccurs="0">
				<xsd:element name="userLogRecord"
						type="tns:UserLogRecord"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="GetUserResult">
		<xsd:sequence>
			<xsd:element minOccurs="0"
					name="userId"
					nillable="true"
					type="tns:alphanumeric50"/>
			<xsd:element name="firstName"
					type="tns:alphanumeric50"/>
			<xsd:element name="lastName"
					type="tns:alphanumeric50"/>
			<xsd:element minOccurs="0"
					name="middleName"
					nillable="true"
					type="tns:alphanumeric50"/>
			<xsd:element minOccurs="0"
					name="position"
					nillable="true"
					type="tns:alphanumericspecials50"/>
			<xsd:element name="login"
					type="tns:login"/>
			<xsd:element minOccurs="0"
					name="password"
					nillable="true"
					type="xsd:string"/>
			<xsd:element minOccurs="0"
					name="role"
					nillable="true"
					type="tns:roleType"/>
			<xsd:element minOccurs="0"
					name="rfid"
					nillable="true"
					type="tns:rfid"/>
			<xsd:element name="active"
					type="xsd:boolean"/>
			<xsd:element minOccurs="0"
					name="lastEntrance"
					nillable="true"
					type="xsd:dateTime"/>
			<xsd:element minOccurs="0"
					name="remoteLogin"
					nillable="true"
					type="tns:login"/>
			<xsd:element minOccurs="0"
					name="remotePassword"
					nillable="true"
					type="xsd:string"/>
			<xsd:element minOccurs="0"
					name="remoteAddress"
					nillable="true"
					type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="UserLogRecord">
		<xsd:sequence>
			<xsd:element maxOccurs="1"
					minOccurs="0"
					name="eventDate"
					nillable="true"
					type="xsd:date"/>
			<xsd:element maxOccurs="1"
					minOccurs="1"
					name="event"
					nillable="false"
					type="tns:alphanumeric50"/>
			<xsd:element maxOccurs="1"
					minOccurs="1"
					name="name"
					nillable="false"
					type="tns:alphanumeric50"/>
			<xsd:element maxOccurs="1"
					minOccurs="0"
					name="role"
					nillable="true"
					type="tns:roleType"/>
			<xsd:element maxOccurs="1"
					minOccurs="1"
					name="login"
					nillable="false"
					type="tns:login"/>
			<xsd:element maxOccurs="1"
					minOccurs="1"
					name="userId"
					nillable="false"
					type="tns:alphanumeric50"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- Order Service -->
	<xsd:element name="UploadStorage">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element maxOccurs="1"
						minOccurs="1"
						name="storageId"
						type="xsd:string"/>
				<xsd:element maxOccurs="1"
						minOccurs="0"
						name="itemId"
						nillable="true"
						type="xsd:string"/>
				<xsd:element maxOccurs="1"
						minOccurs="0"
						name="trayNumber"
						type="xsd:int"/>
				<xsd:element maxOccurs="1"
						minOccurs="0"
						name="poseX"
						nillable="true"
						type="xsd:double"/>
				<xsd:element maxOccurs="1"
						minOccurs="0"
						name="poseY"
						nillable="true"
						type="xsd:double"/>
				<xsd:element maxOccurs="1"
						minOccurs="0"
						name="width"
						nillable="true"
						type="xsd:double"/>
				<xsd:element maxOccurs="1"
						minOccurs="0"
						name="depth"
						nillable="true"
						type="xsd:double"/>
				<xsd:element maxOccurs="1"
						minOccurs="0"
						name="barcode"
						nillable="true"
						type="tns:barcode"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="UploadStorageResponse">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element maxOccurs="1"
						minOccurs="0"
						name="message"
						type="xsd:string"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="GetStorageList">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element maxOccurs="1"
						minOccurs="0"
						name="trayNumber"
						type="xsd:int"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="DeleteStorage">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element maxOccurs="1"
						minOccurs="1"
						name="storageId"
						type="xsd:string"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="GetStorageListResult">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element maxOccurs="1"
						minOccurs="0"
						name="message"
						type="xsd:string"/>
				<xsd:element maxOccurs="unbounded"
						minOccurs="0"
						name="storage"
						type="tns:GetStorageResult"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="DeleteStorageResponse">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element maxOccurs="1"
						minOccurs="0"
						name="message"
						type="xsd:string"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="Storage">
		<xsd:sequence>
			<!--from TRAYS-->
			<xsd:element maxOccurs="1"
					minOccurs="0"
					name="storageId"
					type="xsd:string"/>
			<xsd:element maxOccurs="1"
					minOccurs="0"
					name="trayNumber"
					type="xsd:int"/>
			<!--from ITEMS-->
			<xsd:element maxOccurs="1"
					minOccurs="0"
					name="poseX"
					type="xsd:double"/>
			<xsd:element maxOccurs="1"
					minOccurs="0"
					name="poseY"
					type="xsd:double"/>
			<xsd:element maxOccurs="1"
					minOccurs="0"
					name="depth"
					type="xsd:double"/>
			<xsd:element maxOccurs="1"
					minOccurs="0"
					name="width"
					type="xsd:double"/>
			<xsd:element maxOccurs="1"
					minOccurs="0"
					name="itemId"
					type="xsd:string"/>
			<xsd:element maxOccurs="1"
					minOccurs="0"
					name="quant"
					type="xsd:double"/>
			<xsd:element maxOccurs="1"
					minOccurs="0"
					name="barcode"
					type="tns:barcode"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="GetStorageResult">
		<xsd:sequence>
			<xsd:element minOccurs="0"
					name="storageId"
					nillable="true"
					type="xsd:string"/>
			<xsd:element name="trayNumber"
					type="xsd:int"/>
			<xsd:element name="poseX"
					type="xsd:double"/>
			<xsd:element name="poseY"
					type="xsd:double"/>
			<xsd:element name="depth"
					type="xsd:double"/>
			<xsd:element name="width"
					type="xsd:double"/>
			<xsd:element minOccurs="0"
					name="itemId"
					nillable="true"
					type="xsd:string"/>
			<xsd:element name="quant"
					type="xsd:double"/>
			<xsd:element minOccurs="0"
					name="barcode"
					nillable="true"
					type="tns:barcode"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:simpleType name="uomType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="EACH"/>
			<xsd:enumeration value="SET"/>
			<xsd:enumeration value="PACK"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Led Line Service -->
	<xsd:simpleType name="barcode">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="255"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Led Display Service -->
	<xsd:element name="UploadItemList">
		<xsd:complexType>
			<xsd:sequence maxOccurs="unbounded"
					minOccurs="0">
				<xsd:element name="item"
						type="tns:Item"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="Item">
		<xsd:sequence>
			<xsd:element maxOccurs="1"
					minOccurs="1"
					name="itemId"
					nillable="false"
					type="tns:alphanumericspecials50"/>
			<!-- Код 1С -->
			<xsd:element maxOccurs="1"
					minOccurs="1"
					name="itemName"
					nillable="false"
					type="tns:alphanumericspecials150"/>
			<xsd:element maxOccurs="1"
					minOccurs="1"
					name="unit"
					nillable="false"
					type="tns:uomType"/>
			<xsd:element maxOccurs="1"
					minOccurs="1"
					name="active"
					nillable="false"
					type="xsd:boolean"/>
			<xsd:element maxOccurs="1"
					minOccurs="0"
					name="isDecimal"
					nillable="false"
					type="xsd:boolean"/>
			<xsd:element maxOccurs="1"
					minOccurs="0"
					name="extraCode"
					nillable="false"
					type="tns:alphanumeric20"/>
			<xsd:element maxOccurs="1"
					minOccurs="0"
					name="vendorNumber"
					nillable="false"
					type="tns:alphanumericspecials100"/>
			<xsd:element maxOccurs="1"
					minOccurs="0"
					name="width"
					nillable="true"
					type="xsd:int"/>
			<xsd:element maxOccurs="1"
					minOccurs="0"
					name="height"
					nillable="true"
					type="xsd:int"/>
			<xsd:element maxOccurs="1"
					minOccurs="0"
					name="depth"
					nillable="true"
					type="xsd:int"/>
			<xsd:element maxOccurs="1"
					minOccurs="0"
					name="weight"
					nillable="true"
					type="xsd:int"/>
			<xsd:element maxOccurs="1"
					minOccurs="0"
					name="barcode"
					nillable="true"
					type="tns:barcode"/>
			<xsd:element maxOccurs="1"
					minOccurs="0"
					name="weightCheck"
					nillable="false"
					type="xsd:boolean"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- Tray Service -->
	<xsd:element name="UploadStocksList">
		<xsd:complexType>
			<xsd:sequence maxOccurs="unbounded"
					minOccurs="0">
				<xsd:element name="stock"
						type="tns:Stock"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="Stock">
		<xsd:sequence>
			<xsd:element maxOccurs="1"
					minOccurs="1"
					name="itemId"
					nillable="false"
					type="tns:alphanumericspecials50"/>
			<xsd:element maxOccurs="unbounded"
					minOccurs="1"
					name="trayQty"
					nillable="false"
					type="tns:TrayQuantity"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="TrayQuantity">
		<xsd:sequence>
			<xsd:element maxOccurs="1"
					minOccurs="1"
					name="trayNumber"
					nillable="false"
					type="xsd:int"/>
			<xsd:element maxOccurs="1"
					minOccurs="1"
					name="quantity"
					type="xsd:decimal"/>
			<xsd:element maxOccurs="1"
					minOccurs="1"
					name="storageId"
					type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element name="UploadResult">
		<xsd:complexType>
			<xsd:all>
				<xsd:element name="message"
						type="xsd:string"/>
				<xsd:element name="errorCode"
						nillable="true"
						type="xsd:string"/>
			</xsd:all>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="GetItemList">
		<xsd:complexType>
			<xsd:sequence/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="ItemListResult">
		<xsd:complexType>
			<xsd:sequence maxOccurs="unbounded"
					minOccurs="0">
				<xsd:element name="item"
						type="tns:GetItemResult"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="GetItemResult">
		<xsd:sequence>
			<xsd:element minOccurs="0"
					name="itemId"
					nillable="true"
					type="tns:alphanumericspecials50"/>
			<!-- Код 1С -->
			<xsd:element name="itemName"
					type="tns:alphanumericspecials150"/>
			<xsd:element name="unit"
					type="tns:uomType"/>
			<xsd:element name="active"
					type="xsd:boolean"/>
			<xsd:element name="isDecimal"
					type="xsd:boolean"/>
			<xsd:element minOccurs="0"
					name="extraCode"
					nillable="true"
					type="tns:alphanumeric20"/>
			<xsd:element minOccurs="0"
					name="vendorNumber"
					nillable="true"
					type="tns:alphanumericspecials100"/>
			<xsd:element minOccurs="0"
					name="width"
					type="xsd:int"/>
			<xsd:element minOccurs="0"
					name="height"
					type="xsd:int"/>
			<xsd:element minOccurs="0"
					name="depth"
					type="xsd:int"/>
			<xsd:element minOccurs="0"
					name="weight"
					type="xsd:int"/>
			<xsd:element minOccurs="0"
					name="barcode"
					nillable="true"
					type="tns:barcode"/>
			<xsd:element name="weightCheck"
					type="xsd:boolean"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:simpleType name="balanceType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="TOTAL"/>
			<xsd:enumeration value="STORAGES"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:element name="GetStockBalances">
		<xsd:complexType>
			<xsd:sequence>
